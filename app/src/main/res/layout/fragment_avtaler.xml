<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ListView
        android:id="@+id/listView"
        android:layout_width="match_parent"
        android:layout_height="745dp"
        android:layout_weight="1" />

    <!-- Legg til andre elementer som trengs for fragmentet her -->

    <com.google.android.material.floatingactionbutton.FloatingActionButton
        android:id="@+id/btnLeggTilAvtale"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_gravity="top|end"
        android:layout_margin="40dp"
        android:contentDescription="@string/legg_til_avtale"
        app:srcCompat="@drawable/centered_add_icon"
        app:maxImageSize="80dp"
        />
</FrameLayout>
